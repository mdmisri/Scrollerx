    "description": "A Chrome extension to scroll to the Top or Bottom",
    "permissions": ["activeTab", "scripting"],
    "icons": {
      "48": "icon48.png"
    },
    "action": {
        "default_popup":"popup.html",
      "default_icon": {
        "48": "icon48.png"
      }
    }
  }
  